{
  "mcpServers": {
    "prospect-dashboard": {
      "command": "node",
      "args": [
        "/absolute/path/to/mcp-prospect-dashboard/dist/index.js"
      ],
      "env": {
        "SUPABASE_URL": "${SUPABASE_URL}",
        "SUPABASE_SERVICE_ROLE_KEY": "${SUPABASE_SERVICE_ROLE_KEY}",
        "APP_URL": "${APP_URL:-https://getonapod.com}"
      }
    }
  }
}
